create table SZ_METRO_HIS_DEAL (
CARD_ID            NUMBER(16),
TRADE_TYPE         NUMBER(3),
CARD_TYPE          NUMBER(2),
TRADE_ADDRESS      NUMBER(10),
TERMINAL_ID        NUMBER(9),
TRADE_MONEY        NUMBER(5),
TRADE_VALUE        NUMBER(10),
CURRENT_VALUE      NUMBER(12),
TRADE_DATE        DATE,
DESTINATION      NUMBER(12,2),
RECV_TIME         VARCHAR(16)
);